    "assign": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --assign --url \"$KASM_URL\"'"
    }
  },
  "categories": [
    "Change Detector"
  ],
  "friendly_name": "Change-Detection",
  "architecture": [
    "amd64"
  ],
  "compatibility": [
    "1.13.x",
    "1.14.x"
  ],
  "uncompressed_size_mb": 3030
}