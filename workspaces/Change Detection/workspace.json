{
  "description": "Web page monitoring, notification and change detection",
  "docker_registry": "https://index.docker.io/v1/",
  "image_src": "changedetection.png",
  "name": "replaceme4",
  "run_config": {
    "hostname": "kasm",
    "environment": {
      "LAUNCH_URL": "--app=https://changes.redacted.io"
