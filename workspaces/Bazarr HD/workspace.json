{
  "description": "Companion application to Sonarr and Radarr that manages and downloads subtitles based on your requirements.",
  "docker_registry": "https://index.docker.io/v1/",
  "image_src": "bazarr.png",
  "name": "kasmweb/brave:1.14.0",
  "run_config": {
    "hostname": "kasm",
    "environment": {
      "LAUNCH_URL": "--app=https://bazarr-hd.redacted.io"
    }
  },
  "exec_config": {
    "go": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --go --url \"$KASM_URL\"'"
    },
    "assign": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --assign --url \"$KASM_URL\"'"
    }
  },
  "categories": [
    "Subtitles Fetcher"
  ],
  "friendly_name": "Bazarr (HD)",
  "architecture": [
    "amd64"
  ],
  "compatibility": [
    "1.13.x",
    "1.14.x"
  ],
  "uncompressed_size_mb": 3030
}