{
  "description": "Clean interface for browsing, reading and downloading eBooks",
  "docker_registry": "https://index.docker.io/v1/",
  "image_src": "calibre-web.png",
  "name": "replaceme3",
  "run_config": {
    "hostname": "kasm",
    "environment": {
      "LAUNCH_URL": "--app=https://ebooks.redacted.io"
    }
  },
  "exec_config": {
    "go": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --go --url \"$KASM_URL\"'"
    },
    "assign": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --assign --url \"$KASM_URL\"'"
    }
  },
  "categories": [
    "eBook Manager"
  ],
  "friendly_name": "Calibre Web",
  "architecture": [
    "amd64"
  ],
  "compatibility": [
    "1.13.x",
    "1.14.x"
  ],
  "uncompressed_size_mb": 3030
}